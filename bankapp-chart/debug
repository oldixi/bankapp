# Source: bankapp-chart/charts/accounts/templates/config.yaml
apiVersion: v1
kind: ConfigMap
metadata:
  name: accounts
  namespace: bankapp
data:
  application.yaml: |
    server:
      port: 8082
    spring:
      application:
        name: accounts
      datasource:
        url: "jdbc:postgresql://db:5432/bankapp"
        driverClassName: org.postgresql.Driver
      liquibase:
        url: "jdbc:postgresql://db:5432/bankapp"
        driverClassName: org.postgresql.Driver
      security:
        oauth2:
          client:
            registration:
              keycloak:
                clientId: accounts
                provider: keycloak
                authorizationGrantType: client_credentials
                scope: openid,profile,email
                clientAuthenticationMethod: client_secret_basic
            provider:
              keycloak:
                issuerUri: http://keycloak/realms/bankapp
          resourceserver:
            jwt:
              issuerUri: http://keycloak/realms/bankapp
              jwkSetUri: http://keycloak/realms/bankapp/protocol/openid-connect/certs
    feign:
      circuitbreaker:
        enabled: true
      config:
        default:
          retryer: feign.Retryer.Default
---
