# Source: bankapp-chart/charts/accounts/templates/deployment.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: accounts
  namespace: bankapp
  labels:
    helm.sh/chart: accounts-0.1.0
    app.kubernetes.io/name: accounts
    app.kubernetes.io/instance: bankapp
    app.kubernetes.io/version: "0.0.1"
    app.kubernetes.io/managed-by: Helm
spec:
  replicas: 1
  selector:
    matchLabels:
      app.kubernetes.io/name: accounts
      app.kubernetes.io/instance: bankapp
  template:
    metadata:
      labels:
        helm.sh/chart: accounts-0.1.0
        app.kubernetes.io/name: accounts
        app.kubernetes.io/instance: bankapp
        app.kubernetes.io/version: "0.0.1"
        app.kubernetes.io/managed-by: Helm
    spec:
      serviceAccountName: accounts
      containers:
        - name: accounts
          image: "bankapp/accounts:0.0.1"
          imagePullPolicy: IfNotPresent
          ports:
            - name: http
              containerPort: 8082
              protocol: TCP
          env:
            - name: SPRING_CONFIG_LOCATION
              value: "classpath:/,file:/app/config/application.yaml"
            - name: SPRING_CONFIG_ADDITIONAL_LOCATION
              value: "file:/app/config/"
            - name: SPRING_AUTOCONFIGURE_EXCLUDE
              value: "org.keycloak.adapters.springboot.KeycloakAutoConfiguration,org.keycloak.adapters.springboot.KeycloakSpringBootProperties"
            - name: SPRING_DATASOURCE_USERNAME
              valueFrom:
                secretKeyRef:
                  name: accounts-secrets
                  key: datasource-username
            - name: SPRING_DATASOURCE_PASSWORD
              valueFrom:
                secretKeyRef:
                  name: accounts-secrets
                  key: datasource-password
            - name: SPRING_LIQUIBASE_USER
              valueFrom:
                secretKeyRef:
                  name: accounts-secrets
                  key: liquibase-user
            - name: SPRING_LIQUIBASE_PASSWORD
              valueFrom:
                secretKeyRef:
                  name: accounts-secrets
                  key: liquibase-password
            - name: SPRING_SECURITY_OAUTH2_CLIENT_REGISTRATION_KEYCLOAK_CLIENT_SECRET
              valueFrom:
                secretKeyRef:
                  name: accounts-secrets
                  key: oauth2-client-secret
          volumeMounts:
            - mountPath: /app/config
              name: config-volume
              readOnly: true
      volumes:
        - configMap:
            name: accounts
          name: config-volume
---
