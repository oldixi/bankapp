apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: front-access
  namespace: bankapp
spec:
  podSelector:
    matchLabels:
      app.kubernetes.io/name: front
  policyTypes:
  - Ingress
  ingress:
  - from:
    - namespaceSelector: {}
    ports:
    - protocol: TCP
      port: 8086